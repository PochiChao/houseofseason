generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type EventsCourseOne {
  description String
  dishName    String
  img         String
}

type EventsCourseThree {
  description String
  dishName    String
  img         String
}

type EventsCourseTwo {
  description String
  dishName    String
  img         String
}

model events {
  id          String            @id @default(auto()) @map("_id") @db.ObjectId
  courseOne   EventsCourseOne
  courseThree EventsCourseThree
  courseTwo   EventsCourseTwo
  event       String
}
